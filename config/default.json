{
  "Database": {
    "client": "mysql",
    "connection": {
      "host": "localhost",
      "port": 3306,
      "user": "",
      "password": "",
      "database": "music"
    },
    "pool": {
      "min": 0,
      "max": 20
    },
    "prefix": "music"
  },
  "Server": {
    "protocol": "http",
    "key": "",
    "cert": "",
    "plain": false,
    "behindProxy": false,
    "windowSize": 1048576
  },
  "Folders": {
    "baseDir": "/home/music/songs",
    "folders": [
      "nsp",
      "edm",
      "jingles"
    ]
  },
  "Downloader": {
    "proxy": null,
    "forceIpv4": false,
    "allowPlaylists": true,
    "maxConcurrentDownloads": 1,
    "audioCodec": "libmp3lame"
  },
  "Keys": {
    "lastfm": "",
    "Google": {
      "clientID": "",
      "clientSecret": ""
    },
    "Twitter": {
      "consumerKey": "",
      "consumerSecret": ""
    }
  },
  "General": {
    "baseURL": "",
    "tempDir": "temp/",
    "christmasMode": false,
    "shuffleQueueSize": 100
  },
  "Cookies": {
    "maxAge": 1e10
  },
  "Static": {
    "maxAge": 345600000
  },
  "Advanced": {
    "htmlMinifier": {
      "removeComments": true,
      "collapseWhitespace": true,
      "collapseBooleanAttributes": true,
      "removeAttributeQuotes": true,
      "removeEmptyAttributes": true,
      "minifyJS": true
    },
    "bodyparser": {
      "extended": false
    },
    "session": {
      "secret": "aTotallyTemporarySecret",
      "resave": false,
      "saveUninitialized": true
    },
    "clientOrder": [
      "client/jquery.quickfit.js",
      "client/AppInit.js",
      "client/UpdateManager.js",
      "client/RootScope.js",
      "client/admin/AdminController.js",
      "client/AddController.js",
      "client/AddDeviceController.js",
      "client/AddPlaylistController.js",
      "client/AddToPlaylistController.js",
      "client/ContextMenuController.js",
      "client/PlaylistController.js",
      "client/ModalController.js",
      "client/SongController.js",
      "client/TabController.js",
      "client/SongInfoTabController.js",
      "client/SettingsController.js",
      "client/EditSongController.js"
    ]
  },
  "Routes": [
    "./routes/index",
    "./routes/auth",
    "./routes/search",
    "./routes/image",
    "./routes/api",
    "./routes/api/song",
    "./routes/api/artist",
    "./routes/api/admin",
    "./routes/api/stats",
    "./routes/api/downloader",
    "./routes/templates",
    "./routes/templates/admin",
    "./routes/templates/modals",
    "./routes/templates/songs",
    "./routes/templates/settings",
    "./routes/templates/delete",
    "./routes/templates/add",
    "./routes/templates/stats",
    "./routes/websocket"
  ],
  "WebsocketHandlers": [
    "deviceHandler.js",
    "adminHandler.js",
    "remoteHandler.js"
  ],
  "RateLimiter": {
    "General": {
      "window": 1000,
      "max": 100,
      "delay": 0,
      "headers": true
    },
    "IntensiveActions": {
      "window": 1000,
      "max": 5,
      "delay": 0,
      "headers": true
    },
    "API": {
      "window": 1000,
      "max": 100,
      "delay": 0,
      "headers": true
    }
  }
